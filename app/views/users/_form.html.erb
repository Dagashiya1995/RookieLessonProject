<%= form_for @user do |f| %>
<%= f.label :grade ,'学年'%>
<%= f.text_field :grade %>
<br />

<%= f.label :name, '氏名'%>
<%= f.text_field :name %>
<br />

<%= f.label :snum, '学籍番号'%>
<%= f.text_field :snum %>
<br />

<%= f.label :account, 'アカウント名'%>
<%= f.text_field :account %>
<br />

<%= f.label :password, 'パスワード'%>
<%= f.password_field :password %>
<br />

<%= f.submit submit_text%>
<% end %>
