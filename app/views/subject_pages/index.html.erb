<h1>SubjectPages#index</h1>
<p>Find me in app/views/subject_pages/index.html.erb</p>

<table>
  <thead>
    <th>タイトル</th>
    <th>カテゴリ</th>
    <th>本文</th>
    <th>更新日</th>
    <th>作成日</th>
    <th></th>
  </thead>
  <tbody>
    <% @subject_pages.each do |subject_page| %>
    <tr>
      <td><%= subject_page.title %></td>
      <td><%= subject_page.category %></td>
       <td><%= subject_page.body %></td>
      <td><%= subject_page.updated_at %></td>
      <td><%= subject_page.created_at  %></td>
      <td><%= link_to '編集', edit_subject_page_path(subject_page) %></td>
      <td><%= link_to '削除', subject_page_path(subject_page), method: 'delete' %></td>
    </tr>
    <% end %>
  </tbody>
</table>

